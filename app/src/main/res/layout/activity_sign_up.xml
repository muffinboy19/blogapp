<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.blogapp.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSignup"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_26pxv"
        android:paddingEnd="@dimen/_28pxh"
        android:paddingStart="@dimen/_28pxh"
        android:paddingTop="@dimen/_26pxv" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageDhiWiseblackmonogram"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_59pxh"
            android:layout_width="@dimen/_59pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_dhiwiseblackmonogram}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_dhiwiseblackmonogram"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtTitletext"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_2pxh"
            android:singleLine="true"
            android:text="@{signUpVM.signUpModel.txtTitletext}"
            style="@style/txtPlantagenetCherokee32" tools:text="@string/lbl_register_now"/>
        <LinearLayout android:id="@+id/linearRowgoogleone"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_15pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_10pxv" style="@style/groupStylecornerRadius">
            <ImageView android:id="@+id/imageGoogleOne"
                android:layout_gravity="center_vertical"
                android:layout_height="@dimen/_21pxh"
                android:layout_width="@dimen/_21pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_google1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_google1"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignupwithGoogle"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignupwithGoogle}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_up_with_go"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowfacebookone"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_15pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_10pxv" style="@style/groupStylecornerRadius">
            <ImageView android:id="@+id/imageFacebookOne"
                android:layout_gravity="top"
                android:layout_height="@dimen/_20pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_facebook1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_facebook1"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignupwithFacebook"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignupwithFacebook}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_up_with_fa"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowtwitterone"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_15pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_10pxv" style="@style/groupStylecornerRadius">
            <ImageView android:id="@+id/imageTwitterOne"
                android:layout_gravity="bottom"
                android:layout_height="@dimen/_19pxh"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_twitter1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_twitter1"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignupwithTwitter"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignupwithTwitter}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_up_with_tw"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowappleblacklogo"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_15pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_10pxv" style="@style/groupStylecornerRadius">
            <ImageView android:id="@+id/imageAppleblacklogo"
                android:layout_gravity="top"
                android:layout_height="@dimen/_20pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_appleblacklogo"
                tools:ignore="ContentDescription" tools:src="@drawable/img_appleblacklogo"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignupwithAppleOne"
                android:layout_gravity="bottom"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignupwithAppleOne}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_sign_up_with_ap"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowconfirmation"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:gravity="center_vertical"
                android:id="@+id/txtConfirmation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtConfirmation}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_already_have_an"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignin"
                android:layout_gravity="center_vertical"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignin}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_sign_in"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameGroup3341"
            android:layout_height="wrap_content" android:layout_width="match_parent"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@{signUpVM.signUpModel.txtDescription}"
            style="@style/txtPoppinsregular12_1" tools:text="@string/msg_by_signing_up"/>
    </LinearLayout>
</layout>
